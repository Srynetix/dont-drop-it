; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Ludum Dare 49 - Don't drop it"
run/main_scene="res://scenes/screens/Boot.tscn"
boot_splash/image="res://assets/textures/BlackPixel.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://assets/textures/Icon.png"

[autoload]

GameLoadCache="*res://scenes/GameLoadCache.cs"
SceneTransitioner="*res://addons/nodeext/SceneTransitioner.tscn"

[display]

window/size/height=576
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Walls"
2d_physics/layer_2="Bomb"
2d_physics/layer_3="Zones"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
